M2:FindFirstChild("Buttonfly"):FindFirstChild("activordesc").MouseButton1Click:Connect(function()
	if _G.NorameTAble.cbcFly == false then
		print()
		fly()
		ActiveClcik(M2:FindFirstChild("Buttonfly"):FindFirstChild("activordesc") ,M2:FindFirstChild("Buttonfly") )
		_G.NorameTAble.cbcFly = true
	else
		if 	ch.PrimaryPart:FindFirstChild("FlightForce") then
			ch.PrimaryPart:FindFirstChild("FlightForce"):Destroy()
		end
		for i ,v in pairs(game.Workspace:GetDescendants()) do 
			if v:IsA("Part") or v:IsA("MesPart") then
				v.CanCollide = true
			end
		end
		UnActiveClick(M2:FindFirstChild("Buttonfly"):FindFirstChild("activordesc") ,M2:FindFirstChild("Buttonfly"))
		_G.NorameTAble.cbcFly = false
	end
end)
