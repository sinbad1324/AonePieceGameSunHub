-----------trueClickFunction------
local function ActiveClcik(C1 ,C2)
	C1:TweenPosition(UDim2.new(0.558 , 0,0.11,0),1)
	C2.BackgroundColor3 = Color3.new(0.541176, 0.533333, 0.811765)
	wait()
end
-----------trueClickFunction------
local function UnActiveClick(C1, C2)
	C1:TweenPosition(UDim2.new(0.084 , 0,0.11,0) , 1)
	C2.BackgroundColor3 = Color3.new(0.443137, 0.443137, 0.443137)
	wait()
end
--------------------ActiveSkill-----------
local function ActiveSkill(S1,S2,F1)
	F1.BackgroundColor3 = Color3.new(0.266667, 0.266667, 0.337255)
	local args = {
		[1] = S1,
		[2] = S2,
		[3] = CFrame.new(0,0,0) * CFrame.Angles(0,0,0),
		[4] = ch.HumanoidRootPart,
		[5] = 5
	}
	game:GetService("ReplicatedStorage").Remotes.requestAbility:FireServer(unpack(args))
end
--------------------ActiveSkill-----------
--------------------UNActiveSkill-----------
local function UNActiveSkill(F1)
	F1.BackgroundColor3 = Color3.new(0.160784, 0.160784, 0.203922)

end
