M2:FindFirstChild("ButtonNoclip"):FindFirstChild("activordesc").MouseButton1Click:Connect(function()
	if _G.NorameTAble.cbcNoclip == false then
		if _G.NorameTAble.cbcFly == true  then
			for i,v in pairs(game.Workspace:GetDescendants()) do
				if v:IsA("Part") or v:IsA("MeshPart") then
					v.CanCollide = false
				end
			end 
		end
		ActiveClcik(M2:FindFirstChild("ButtonNoclip"):FindFirstChild("activordesc") ,M2:FindFirstChild("ButtonNoclip") )
		_G.NorameTAble.cbcNoclip = true
	else
		for i,v in pairs(game.Workspace:GetDescendants()) do
			if v:IsA("Part") or v:IsA("MeshPart") then
				v.CanCollide = true
			end
		end
		UnActiveClick(M2:FindFirstChild("ButtonNoclip"):FindFirstChild("activordesc") ,M2:FindFirstChild("ButtonNoclip"))
		_G.NorameTAble.cbcNoclip = false
	end
end)
