			R4:FindFirstChild("Auto..."):FindFirstChild("ImageLabel").Rotation = 180
		else
			wait(1)
			_G.NorameTAble.OpeAre = false
			R4:FindFirstChild("Auto..."):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,0,0),0.1)	
			R4:FindFirstChild("Auto..."):FindFirstChild("ImageLabel").Rotation = 0
		end	
	end)
	if game.Workspace:FindFirstChild("ArenaZones")  then

	local Arena = {}
	for i ,v in pairs(game.Workspace.ArenaZones:GetChildren()) do
		if v:IsA("Part")	 and v.ClassName == "Part" then
			table.insert(Arena , v)
		end
	end

	for i , v in pairs(Arena) do
		local clone = R4:FindFirstChild("Auto..."):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):FindFirstChild("Listeisland"):FindFirstChild("Quel"):Clone()
		clone.Name = v.Name
		clone.Parent = R4:FindFirstChild("Auto..."):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):FindFirstChild("Listeisland")
		clone.Visible = true
		clone.Text = v.Name
		clone.MouseButton1Click:Connect(function()
			R4:FindFirstChild("Auto..."):FindFirstChild("statsname").Text = clone.Text
			wait(0.5)
			_G.NorameTAble.OpeAre = false
			R4:FindFirstChild("Auto..."):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,0,0),0.1)	
			R4:FindFirstChild("Auto..."):FindFirstChild("ImageLabel").Rotation = 0
			_G.SaTAble.cbcRaid = clone.Text
			saveSetting()
		end)
	end

	R4:FindFirstChild("ButtonsAuto...."):FindFirstChild("activordesc").MouseButton1Click:Connect(function()
		if _G.SaTAble.Laquel.arena == false then
			wait(0.5)
			_G.SaTAble.Laquel.arena = true
			saveSetting()
			ActiveClcik(R4:FindFirstChild("ButtonsAuto...."):FindFirstChild("activordesc") ,R4:FindFirstChild("ButtonsAuto....") )	
