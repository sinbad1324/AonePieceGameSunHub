---------------INFDASH--------------
M6:FindFirstChild("INFdash"):FindFirstChild("activordesc").MouseButton1Click:Connect(function() 
	if _G.NorameTAble.cbcINFdash == false  then
		local inf = "Dash"
		local func 
		for i ,v in pairs(getgc()) do
			if type(v) == "function" and not is_synapse_function(v) and getinfo(v).name then
				if getinfo(v).name == inf then
					func = v
				end
			end
		end
		ch["Dash Handler"].Enabled = false
		-------KeyCodeDBC--------------
		local w = false
		local s = false
		local a = false
		local d = false
		local space = false
		-------KeyCodeDBC--------------		
		ActiveClcik(M6:FindFirstChild("INFdash"):FindFirstChild("activordesc") ,M6:FindFirstChild("INFdash") )

		mouse.KeyDown:connect(function(key)	
			---------W
			if key == "w" then
				if w == false then
					w = true
					wait(0.2)
					w = false
				else
					ch["Dash Handler"].Enabled = false
					setconstant(func , 4 , "W") 
					if func then
						func(Enum.KeyCode.W)
					end
					w = false
				end

				-----S
			elseif key == "s" then
				if s == false then
					s = true
					wait(0.2)
					s = false
				else
					ch["Dash Handler"].Enabled = false
					setconstant(func , 4 , "S") --v20
					if func then
						func(Enum.KeyCode.S)
					end
					s = false
				end

				---------A
			elseif key == "a" then
				if a == false then
					a = true
					wait(0.2)
					a = false
				else
					ch["Dash Handler"].Enabled = false
					setconstant(func , 4 , "A") 
					if func then
						func(Enum.KeyCode.A)
					end
					a = false
				end

				---------D
			elseif key == "d" then
				if d == false then
					d = true
					wait(0.2)
					d = false
				else
					ch["Dash Handler"].Enabled = false
					setconstant(func , 4 , "D") 
					if func then
						func(Enum.KeyCode.D)
					end
					d = false
				end
				---------Space
			elseif key == " " then
				if space == false then
					space = true
					wait(0.2)
					space = false
				else
					ch["Dash Handler"].Enabled = false
					setconstant(func , 4 , "Space") 
					if func then
						func(Enum.KeyCode.Space)
					end
					space = false
				end
			end
		end)
		_G.NorameTAble.cbcINFdash = true
	else
		ch:FindFirstChild("Dash Handler").Enabled = true
		_G.NorameTAble.cbcINFdash = false
		UnActiveClick(M6:FindFirstChild("INFdash"):FindFirstChild("activordesc") ,M6:FindFirstChild("INFdash") )
	end
end)
