		M5:FindFirstChild("SwordUse"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,0,0),0.1)	
		M5:FindFirstChild("MelleUse"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,0,0),0.1)	
		_G.NorameTAble.OpMelle = false
		M5:FindFirstChild("MelleUse").Visible = false
		M5:FindFirstChild("SwordUse").Visible = false
		M5:FindFirstChild("FruitUse"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,1,0),0.1)	
		_G.NorameTAble.Opfruse = true
	else
		wait(0.5)
		_G.NorameTAble.Opfruse = false
		M5:FindFirstChild("FruitUse"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,0,0),0.1)	
		M5:FindFirstChild("MelleUse").Visible = true
		M5:FindFirstChild("SwordUse").Visible = true
	end
end)
