		_G.SaTAble.SaMelleKey.MelleG = true
		spawn(function()
			while _G.SaTAble.SaMelleKey.MelleG do 
				if _G.SaTAble.SaMelleKey.MelleG == true then
					ActiveSkill("Fighting Style" ,"G" , M5:FindFirstChild("MelleUse"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):FindFirstChild("Listeisland"):FindFirstChild("G"))
				end
				wait(1.5)
			end
		end)
		saveSetting()
	else
		_G.SaTAble.SaMelleKey.MelleG = false
		UNActiveSkill(M5:FindFirstChild("MelleUse"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):FindFirstChild("Listeisland"):FindFirstChild("G"))
		saveSetting()
	end
end)
