	if 	_G.NorameTAble.cbcAutoFruitBuy == false then
		_G.NorameTAble.cbcAutoFruitBuy = true
		ActiveClcik(M4:FindFirstChild("AutoFruitBuy"):FindFirstChild("activordesc") , M4:FindFirstChild("AutoFruitBuy"))
		spawn(function()
			while _G.NorameTAble.cbcAutoFruitBuy do
				if 	_G.NorameTAble.cbcAutoFruitBuy == true then
					local args = {
						[1] = "1"
					}
					game:GetService("ReplicatedStorage").Remotes.buyrngdf:FireServer(unpack(args))
				end
				wait()
			end
		end)
	else
		_G.NorameTAble.cbcAutoFruitBuy = false
		UnActiveClick(M4:FindFirstChild("AutoFruitBuy"):FindFirstChild("activordesc") , M4:FindFirstChild("AutoFruitBuy"))
	end
end)
