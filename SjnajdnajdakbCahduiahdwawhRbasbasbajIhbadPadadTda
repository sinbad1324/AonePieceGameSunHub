local quel = P3:FindFirstChild("SetelectTool"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame").Listeisland.Tool
P3:FindFirstChild("SetelectTool"):FindFirstChild("click").MouseButton1Click:Connect(function()
	if _G.NorameTAble.Opequiptool == false then
		_G.NorameTAble.Opequiptool = true
		wait(1)
		Playerscroll.CanvasSize = UDim2.new(0,0,2.3,0)
		PlayerFramm:FindFirstChild("Deco").Visible = false
		P3:FindFirstChild("SetelectTool"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,1,0),0.1)
		P3:FindFirstChild("SetelectTool"):FindFirstChild("ImageLabel").Rotation = 180
		wait(0.1)
		local toolTable  = {}
		for i , v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			if v:FindFirstChild("Handle") then
				---r
			else
				if v:IsA("Tool") then
					table.insert(toolTable , v )
				end
			end
		end 

		for i ,v in pairs(toolTable) do 
			local clone = quel:Clone()
			clone.Text = v.Name
			clone.Parent = quel.Parent
			clone.Name = v.Name
			clone.Visible = true
			clone.MouseButton1Click:Connect(function()
				P3:FindFirstChild("SetelectTool"):FindFirstChild("statsname").Text = clone.Text
				_G.SaTAble.Item = P3:FindFirstChild("SetelectTool"):FindFirstChild("statsname").Text
				saveSetting()
				saveSetting()
				wait(0.5)
				_G.NorameTAble.Opequiptool = false
				PlayerFramm:FindFirstChild("Deco").Visible = true
				P3:FindFirstChild("SetelectTool"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,0,0),0.1)	
				P3:FindFirstChild("SetelectTool"):FindFirstChild("ImageLabel").Rotation = 0
				for i , v in pairs(P3:FindFirstChild("SetelectTool"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame").Listeisland:GetChildren()) do
					if v.ClassName == "TextButton" and v ~= quel then
						v:Destroy()
					end
				end
			end)
		end
	else
		for i , v in pairs(P3:FindFirstChild("SetelectTool"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame").Listeisland:GetChildren()) do
			if v.ClassName == "TextButton" and v ~= quel then
				v:Destroy()
			end
		end
		wait(1)
		_G.NorameTAble.Opequiptool = false
		PlayerFramm:FindFirstChild("Deco").Visible = true
		P3:FindFirstChild("SetelectTool"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,0,0),0.1)	
		P3:FindFirstChild("SetelectTool"):FindFirstChild("ImageLabel").Rotation = 0
		for i , v in pairs(P3:FindFirstChild("SetelectTool"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame").Listeisland:GetChildren()) do
			if v.ClassName == "TextButton" and v ~= quel then
				v:Destroy()
			end
		end 
	end	
end)
P3:FindFirstChild("buttonEquipe"):FindFirstChild("activordesc").MouseButton1Click:Connect(function()
	if _G.SaTAble.cbequipe == false then
		wait(0.5)
		_G.SaTAble.cbequipe = true
		saveSetting()
		ActiveClcik(P3:FindFirstChild("buttonEquipe"):FindFirstChild("activordesc") ,P3:FindFirstChild("buttonEquipe") )
		spawn(function()
			while 	_G.SaTAble.cbequipe  do
				if _G.SaTAble.cbequipe  == true then
					for i , v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
						if v:FindFirstChild("Handle") then
							---r
						else
							if v:IsA("Tool") then
								if v.Name == P3.SetelectTool.statsname.Text then
		                         if v then
										game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
									end
								end
							end
						end
					end 
				end
				wait(0.5)
			end
		end)		
		game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(char2)
			ch = char2
			wait()
			spawn(function()
				while 	_G.SaTAble.cbequipe  do
					if _G.SaTAble.cbequipe  == true then
						for i , v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
							if v:FindFirstChild("Handle") then
								---r
							else
								if v:IsA("Tool") then
									if v.Name == P3.SetelectTool.statsname.Text then
										if v then
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
							end
						end 
					end
					wait(0.5)
				end
			end)		
		end)

	else

		wait(0.5)
		_G.SaTAble.cbequipe = false
		saveSetting()
		UnActiveClick(P3:FindFirstChild("buttonEquipe"):FindFirstChild("activordesc") ,P3:FindFirstChild("buttonEquipe") )
	end	
end)
