	ActiveClcik(M1:FindFirstChild("Buttonspeed"):FindFirstChild("activordesc") ,M1:FindFirstChild("Buttonspeed"))
	if ch then
		local hum = ch.Humanoid
		chnagespeed = hum.Changed:Connect(function()
			hum.WalkSpeed = _G.SaTAble.valuepedd
			for i , v in pairs(ch:GetChildren()) do
				if v:IsA("Part") or v:IsA("MeshPart") then
					v.Anchored = false
				end
			end
		end)
		hum.WalkSpeed = _G.SaTAble.valuepedd
		for i , v in pairs(ch:GetChildren()) do
			if v:IsA("Part") or v:IsA("MeshPart") then
				v.Anchored = false
			end
		end
	end
end
