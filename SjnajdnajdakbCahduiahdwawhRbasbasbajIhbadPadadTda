---------------------------------Raid--------------------------
local function xframeraid(npc , tim)
	if ch.Humanoid.Health >= _G.SaTAble.RaidHP then
		inf = TweenInfo.new(((ch.HumanoidRootPart.Position - npc.Position).Magnitude) / tim)
		twen = tw:Create(ch.HumanoidRootPart , inf , {CFrame = npc * CFrame.new(0,_G.SaTAble.RaidDiste,0) * CFrame.Angles(4.5,0,0)})
		if not ch.HumanoidRootPart:FindFirstChild("sinbad") then
			body = Instance.new("BodyVelocity" , ch.HumanoidRootPart)
			body.Name = "sinbad"
			body.Velocity = Vector3.new(0,0,0)
			body.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
			nop = game:GetService("RunService").Stepped:Connect(nocpl)
			twen:Play()
		end
		twen.Completed:Connect(function ()
			if ch.HumanoidRootPart:FindFirstChild("sinbad") then
				ch.HumanoidRootPart:FindFirstChild("sinbad"):Destroy()
			end
			nop:Disconnect()
		end)
	end
	if   _G.SaTAble.cbcsavezone == true then
		if ch.Humanoid.Health <= (_G.SaTAble.RaidHP - 1) then
			inf = TweenInfo.new(((ch.HumanoidRootPart.Position - npc.Position).Magnitude) / tim)
			twen2 = tw:Create(ch.HumanoidRootPart , inf , {CFrame = npc * CFrame.new(0,300,0) * CFrame.Angles(4.5,0,0)})
			if not ch.HumanoidRootPart:FindFirstChild("sinbad") then
				body = Instance.new("BodyVelocity" , ch.HumanoidRootPart)
				print(1)

				body.Name = "sinbad"
				body.Velocity = Vector3.new(0,0,0)
				body.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
				nop = game:GetService("RunService").Stepped:Connect(nocpl)
				twen2:Play()
			end
			twen2.Completed:Connect(function ()
				if ch.HumanoidRootPart:FindFirstChild("sinbad") then
					ch.HumanoidRootPart:FindFirstChild("sinbad"):Destroy()
				end
				nop:Disconnect()
			end)
		end

	end
end
