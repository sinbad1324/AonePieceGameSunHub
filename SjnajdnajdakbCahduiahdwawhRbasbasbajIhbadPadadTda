--------autoclik-----------
local function Autoclick(S1 , S2)
	local args = {
		[1] = S1,
		[2] = S2,
		[3] = CFrame.new(0,0,0) * CFrame.Angles(0,0,0),
		[4] = ch.HumanoidRootPart,
		[5] = 5
	}
	game:GetService("ReplicatedStorage").Remotes.requestAbility:FireServer(unpack(args))
end
------------autoclick------------
-------------------FlyFunction!----------------
local function fly()
	local flying = false
	local wPressed = false
	local sPressed = false
	local aPressed = false
	local dPressed = false
	uis.InputBegan:Connect(function(key, chat)
		if chat then return end
		if key.KeyCode ==  Enum.KeyCode.H  then
			if flying  then 
				flying = false
			else 
				flying = true
				local bv = Instance.new("BodyVelocity", ch.PrimaryPart)
				bv.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
				bv.Velocity = Vector3.new(0,0,0)
				bv.Name = "FlightForce"
				repeat wait(0.1) until flying == false
				ch.PrimaryPart.FlightForce:Destroy()
				for i ,v in pairs(game.Workspace:GetDescendants()) do 
					if v:IsA("Part") or v:IsA("MesPart") then
						v.CanCollide = true
					end
				end
				bv:Destroy()
			end
		end

		if key.KeyCode == Enum.KeyCode.W then
			wPressed = true
		elseif key.KeyCode == Enum.KeyCode.S then
			sPressed = true
		elseif key.KeyCode == Enum.KeyCode.A then
			aPressed = true
		elseif key.KeyCode == Enum.KeyCode.D then
			dPressed = true
		end
	end)

	uis.InputEnded:Connect(function(key)	
		if key.KeyCode == Enum.KeyCode.W then
			wPressed = false
		elseif key.KeyCode == Enum.KeyCode.S then
			sPressed = false
		elseif key.KeyCode == Enum.KeyCode.A then
			aPressed = false
		elseif key.KeyCode == Enum.KeyCode.D then
			dPressed = false
		end
	end)
	spawn(function()
		game["Run Service"].Stepped:Connect(function()
			if flying == true then
				ch.PrimaryPart:FindFirstChild("FlightForce").Velocity = Vector3.new(0,0,0)
				if wPressed then
					ch.PrimaryPart:FindFirstChild("FlightForce").Velocity = cam.CFrame.LookVector * _G.NorameTAble.valueFly
				end
				if sPressed then
					ch.PrimaryPart:FindFirstChild("FlightForce").Velocity = cam.CFrame.LookVector * -_G.NorameTAble.valueFly	
				end
				if aPressed then
					ch.PrimaryPart:FindFirstChild("FlightForce").Velocity = cam.CFrame.RightVector * -_G.NorameTAble.valueFly		
				end
				if dPressed then
					ch.PrimaryPart:FindFirstChild("FlightForce").Velocity = cam.CFrame.RightVector * _G.NorameTAble.valueFly
				end
			else
				wait(1)
			end
		end)
	end)
end
-------------------FlyFunction!----------------
-------------AutoDefenceFunction-------------------

local function TeleportAll(Place)
	if ch:FindFirstChild("HumanoidRootPart") then
		if 	ch:FindFirstChild("HumanoidRootPart").CFrame == Place.CFrame then
		else
			ch:FindFirstChild("HumanoidRootPart").CFrame = Place.CFrame
		end
		local body = ch:FindFirstChildOfClass("BodyVelocity")
		if not body then
			body = Instance.new("BodyVelocity", ch:FindFirstChild("HumanoidRootPart"))
			body.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
		end
		body.Velocity = Vector3.new(0, 0, 0)
	end
end
----Second
local function GoToSwim()
	spawn(function()
		while Swim do
			TeleportAll(game:GetService("Workspace").Map.Ocean["Ocean Floor"])
			wait()
		end
	end)
	game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(char)
		ch = char
		wait()
		TeleportAll(game:GetService("Workspace").Map.Ocean["Ocean Floor"])
		spawn(function()
			while Swim do
				if Swim then
					TeleportAll(game:GetService("Workspace").Map.Ocean["Ocean Floor"])
					wait()
				end
			end
		end)
	end)
end
-------------AutoDefenceFunction-------------------

local function whenkill()
	game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(char)
		ch = char
		AutoStatsFarm()()
		wait()
		spawn(function()
			while autoattaque == true do
				if autoattaque == true then
					AutoStatsFarm()
					wait()
				end
			end
		end)
	end)
end

--------------------AutoFarme-----------------------

local nop = nil
local function nocpl()
	for i, a in pairs(ch:GetDescendants()) do
		if a:IsA("BasePart") and a.CanCollide == true then
			a.CanCollide = false
		end
	end
end
local twen 
local twen2 = false
local inf = false
local body
local function xframer(npc , tim)
	local	ch = game.Players.LocalPlayer.Character
	if not npc then
	elseif npc then

		inf = TweenInfo.new(((ch.HumanoidRootPart.Position - npc.Position).Magnitude) / tim)
		twen = tw:Create(ch.HumanoidRootPart , inf , {CFrame = npc * CFrame.new(0,_G.SaTAble.ladis,0) * CFrame.Angles(4.5,0,0)})
		if not ch.HumanoidRootPart:FindFirstChild("sinbad") then
			body = Instance.new("BodyVelocity" , ch.HumanoidRootPart)
			body.Name = "sinbad"
			body.Velocity = Vector3.new(0,0,0)
			body.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
			nop = game:GetService("RunService").Stepped:Connect(nocpl)
			twen:Play()
		end
		twen.Completed:Connect(function ()
			if ch.HumanoidRootPart:FindFirstChild("sinbad") then
				ch.HumanoidRootPart:FindFirstChild("sinbad"):Destroy()
			end
			nop:Disconnect()
		end)
	end
end

-------------------telepor----------------
local twen2 
local twen22 = false
local inf2 = false
local body2
local function xframer2(npc , tim)
	local	ch = game.Players.LocalPlayer.Character
	inf2 = TweenInfo.new(((ch.HumanoidRootPart.Position - npc.Position).Magnitude) / tim)
	twen2 = tw:Create(ch.HumanoidRootPart , inf2 , {CFrame = npc * CFrame.new(0,0,0)})
	if not ch.HumanoidRootPart:FindFirstChild("sinbad") then
		body2 = Instance.new("BodyVelocity" , ch.HumanoidRootPart)
		body2.Name = "sinbad"
		body2.Velocity = Vector3.new(0,0,0)
		body2.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
		nop = game:GetService("RunService").Stepped:Connect(nocpl)
		twen2:Play()
	end
	twen2.Completed:Connect(function ()
		if ch.HumanoidRootPart:FindFirstChild("sinbad") then
			ch.HumanoidRootPart:FindFirstChild("sinbad"):Destroy()
		end
		nop:Disconnect()
	end)
end
