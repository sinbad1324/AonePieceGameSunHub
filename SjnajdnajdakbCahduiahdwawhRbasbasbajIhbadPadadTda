	--------------------ActivationdeAutoFamr-------------------------
	local function NPC(mob)
		local dist, thing = math.huge
		for i, v in pairs(game:GetService("Workspace").Entities:GetChildren()) do
			if v.Name == mob then
				local posi = (ch.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude
				if posi < dist then 
					dist = posi 
					thing = v 
				end
			end
		end
		return thing
	end
	local function NPC1(mob)
		local dist, thing = math.huge
		for i, v in pairs( game.ReplicatedStorage:GetChildren()) do
			if v.Name == mob then
				local posi = (ch.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude
				if posi < dist then 
					dist = posi 
					thing = v 
				end
			end
		end
		return thing
	end
	A1:FindFirstChild("ButtonsAutoFarm"):FindFirstChild("activordesc").MouseButton1Click:Connect(function()
		if _G.SaTAble.AutoMob == false then
			wait(0.5)
			_G.SaTAble.AutoMob = true
			game.Workspace.Gravity = 0
			game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
			game.Players.LocalPlayer.Character.HumanoidRootPart.RotVelocity = Vector3.new(0,0,0)
			local po 
			local here
			------------------toutEstDedans..................---------------
			spawn(function()
				while _G.SaTAble.AutoMob do
					if AutoFarmFrame  then
						local qui = game:GetService("CoreGui").SunHub.Menu.AutoFarmScroll.AutoFarm.A1.AutoFarm.statsname.Text
						here = game.ReplicatedStorage:FindFirstChild(qui)
						if here == nil then	
						elseif here then
							po = (ch.HumanoidRootPart.Position - here:FindFirstChild("HumanoidRootPart").Position).magnitude
						end
						if  here == nil then
							pcall(function()
								local toi = game:GetService("CoreGui").SunHub.Menu.AutoFarmScroll.AutoFarm.A1.AutoFarm.statsname.Text
								xframer(NPC(toi).HumanoidRootPart.CFrame , 500)
							end)
						elseif po <= 2000 then
							pcall(function()
								local toi = game:GetService("CoreGui").SunHub.Menu.AutoFarmScroll.AutoFarm.A1.AutoFarm.statsname.Text
								xframer(NPC(toi).HumanoidRootPart.CFrame , 500)
							end)
						elseif po >= 500 then
							if here == nil then	
							elseif here then
								xframer(here.HumanoidRootPart.CFrame , 500)
							end
						end
						wait()
					end
				end
			end)
			game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(char2)
				ch = char2
				
				wait()
				spawn(function()
					while _G.SaTAble.AutoMob do
						if AutoFarmFrame  then
							local qui = game:GetService("CoreGui").SunHub.Menu.AutoFarmScroll.AutoFarm.A1.AutoFarm.statsname.Text
							here = game.ReplicatedStorage:FindFirstChild(qui)
							if here == nil then	
							elseif here then
								po = (ch.HumanoidRootPart.Position - here:FindFirstChild("HumanoidRootPart").Position).magnitude
							end
							if  here == nil then
								pcall(function()
									local toi = game:GetService("CoreGui").SunHub.Menu.AutoFarmScroll.AutoFarm.A1.AutoFarm.statsname.Text
									xframer(NPC(toi).HumanoidRootPart.CFrame , 500)
								end)
							elseif po <= 300 then
								pcall(function()
									local toi = game:GetService("CoreGui").SunHub.Menu.AutoFarmScroll.AutoFarm.A1.AutoFarm.statsname.Text
									xframer(NPC(toi).HumanoidRootPart.CFrame , 500)
								end)
							elseif po >= 500 then
								if here == nil then	
								elseif here then
									xframer(here.HumanoidRootPart.CFrame , 500)
								end
							end
							wait()
						end
					end
				end)
			end)

			------------------toutEstDedans..................---------------
			ActiveClcik(A1:FindFirstChild("ButtonsAutoFarm"):FindFirstChild("activordesc") ,A1:FindFirstChild("ButtonsAutoFarm") )
			saveSetting()
		else
			wait(0.5)
			game.Workspace.Gravity = 196
			for i, a in pairs(ch:GetDescendants()) do
				if a:IsA("BasePart") and a.CanCollide == false then
					a.CanCollide = true
				end
			end
			UnActiveClick(A1:FindFirstChild("ButtonsAutoFarm"):FindFirstChild("activordesc") ,A1:FindFirstChild("ButtonsAutoFarm") )
			_G.SaTAble.AutoMob = false
			saveSetting()
		end
	end)
	--------------------ActivationdeAutoFamr-------------------------
	-------------------ButtonAutoCklicker---------------
	A4:FindFirstChild("autoclick"):FindFirstChild("activordesc").MouseButton1Click:Connect(function()
		if _G.SaTAble.cbcAutoCkick == false then
			wait(0.5)
			_G.SaTAble.cbcAutoCkick = true
			ActiveClcik(A4:FindFirstChild("autoclick"):FindFirstChild("activordesc") ,A4:FindFirstChild("autoclick") )
			spawn(function()
				while _G.SaTAble.cbcAutoCkick do 
					Autoclick("Sword Style" , "MouseButton1"  )
					Autoclick("Fighting Style" , "MouseButton1"  )		
					Autoclick("Devil Fruit" , "MouseButton1" )
					wait(0.2)
				end
			end)
			saveSetting()
		else
			wait(0.5)
			_G.SaTAble.cbcAutoCkick = false
			saveSetting()
			UnActiveClick(A4:FindFirstChild("autoclick"):FindFirstChild("activordesc") ,A4:FindFirstChild("autoclick") )
		end
	end)
