---------------ButtonSpeed--------------------
M1:FindFirstChild("Buttonspeed"):FindFirstChild("activordesc").MouseButton1Click:Connect(function()
	if _G.SaTAble.cbcSpeed == false then
		print()
		if ch then
			local hum = ch.Humanoid
			chnagespeed = hum.Changed:Connect(function()
				hum.WalkSpeed = _G.SaTAble.valuepedd
				ch.HumanoidRootPart.Anchored = false
			end)
			hum.WalkSpeed = _G.SaTAble.valuepedd
		end
		saveSetting()
		ActiveClcik(M1:FindFirstChild("Buttonspeed"):FindFirstChild("activordesc") ,M1:FindFirstChild("Buttonspeed"))
		_G.SaTAble.cbcSpeed = true
	else

		chnagespeed:Disconnect()
		local hum = ch.Humanoid
		hum.WalkSpeed = 16 
		saveSetting()
		UnActiveClick(M1:FindFirstChild("Buttonspeed"):FindFirstChild("activordesc") ,M1:FindFirstChild("Buttonspeed"))
		_G.SaTAble.cbcSpeed = false
	end
end)
