local run 

P1:FindFirstChild("PlayerEsp"):FindFirstChild("activordesc").MouseButton1Click:Connect(function()
	if _G.NorameTAble.cbcPlayerEsp == false then
		_G.NorameTAble.cbcPlayerEsp = true
		wait(0.5)
		ActiveClcik(P1:FindFirstChild("PlayerEsp"):FindFirstChild("activordesc") ,P1:FindFirstChild("PlayerEsp") )
		local BillboardGui1 = Instance.new("BillboardGui")
		BillboardGui1["LightInfluence"] = 0
		BillboardGui1["Name"] = "PlayerEspSinbad"
		BillboardGui1["Active"] = true
		BillboardGui1["StudsOffset"] = Vector3.new(0, 5, 0)
		BillboardGui1["Size"] = UDim2.new(0, 50, 0, 20)
		BillboardGui1.AlwaysOnTop = true
		BillboardGui1.MaxDistance = math.huge
		----BillboardGui1----
		--Frame2--
		local Frame2 = Instance.new("Frame",BillboardGui1)
		Frame2["Size"] = UDim2.new(1.29999995, 0, 1, 0)
		Frame2["BackgroundTransparency"] = 1
		Frame2["BackgroundColor3"] = Color3.new(1, 1, 1)
		----Frame2----
		--TextLabel3--
		local TextLabel3 = Instance.new("TextLabel",Frame2)
		TextLabel3["TextWrapped"] = true
		TextLabel3["TextColor3"] = Color3.new(0.0980392, 0.0862745, 0.133333)
		TextLabel3["Text"] = "PlayerName"
		TextLabel3["TextStrokeTransparency"] = 0.8999999761581421
		TextLabel3["Font"] = Enum.Font.Jura
		TextLabel3["Name"] = "PlayerName"
		TextLabel3["Position"] = UDim2.new(-0.5, 0, 0, 0)
		TextLabel3["Size"] = UDim2.new(1, 0, 0.5, 0)
		TextLabel3["BackgroundTransparency"] = 2
		TextLabel3["TextSize"] = 10
		TextLabel3["BackgroundColor3"] = Color3.new(1, 1, 1)
		----TextLabel3----
		--TextLabel4--
		local TextLabel4 = Instance.new("TextLabel",Frame2)
		TextLabel4["TextWrapped"] = true
		TextLabel4["TextColor3"] = Color3.new(1, 0.0235294, 0.0392157)
		TextLabel4["Text"] = "Health"
		TextLabel4["TextSize"] = 14
		TextLabel4["Font"] = Enum.Font.RobotoMono
		TextLabel4["Name"] = "Health"
		TextLabel4["Position"] = UDim2.new(-0.5, 0, 0.5, 0)
		TextLabel4["Size"] = UDim2.new(1, 0, 0.5, 0)
		TextLabel4["BackgroundTransparency"] = 1
		TextLabel4["TextScaled"] = true
		TextLabel4["BackgroundColor3"] = Color3.new(1, 1, 1)
		----TextLabel4----
		--TextLabel5--
		local TextLabel5 = Instance.new("TextLabel",Frame2)
		TextLabel5["TextWrapped"] = true
		TextLabel5["TextColor3"] = Color3.new(0, 0, 0)
		TextLabel5["TextStrokeColor3"] = Color3.new(0.541176, 0.541176, 0.541176)
		TextLabel5["Text"] = "10000.19"
		TextLabel5["TextStrokeTransparency"] = 0.699999988079071
		TextLabel5["Font"] = Enum.Font.Jura
		TextLabel5["Name"] = "Distance"
		TextLabel5["Position"] = UDim2.new(0.3, 0, -0.100000001, 0)
		TextLabel5["Size"] = UDim2.new(1, 0, 1, 0)
		TextLabel5["BackgroundTransparency"] = 1
		TextLabel5["TextSize"] = 21
		TextLabel5["BackgroundColor3"] = Color3.new(1, 1, 1)
		----TextLabel5----

		for i ,v in pairs(game.Players:GetPlayers()) do
			if v ~= player and v.Character:FindFirstChild("HumanoidRootPart") then
				run = game:GetService("RunService").RenderStepped:Connect(function()
					if 	_G.NorameTAble.cbcPlayerEsp then
						if not  v.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("PlayerEspSinbad") then
							TextLabel3.Text = v.Name
							TextLabel4.Text = v.Character.Humanoid.Health
							spawn(function()
								v.Character.Humanoid.HealthChanged:Connect(function(health)
									TextLabel4.Text = health
								end)
							end)
							local clone = BillboardGui1:Clone()
							clone.Parent = v.Character:FindFirstChild("HumanoidRootPart")
							spawn(function()
								while true do
									local po = (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - v.Character:FindFirstChild("HumanoidRootPart").Position).magnitude
									v.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("PlayerEspSinbad"):FindFirstChild("Frame"):FindFirstChild("Distance").Text = po
									wait()
								end
							end)
						end
					end
				end)
			end
		end
	else
		wait(0.5)
		_G.NorameTAble.cbcPlayerEsp = false
		run:Disconnect()
		for i , v in pairs(game.Players:GetPlayers()) do
			if v ~= player and v.Character:FindFirstChild("HumanoidRootPart") then
				if v.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("PlayerEspSinbad") then
					v.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("PlayerEspSinbad"):Destroy()
				end
			end
		end
		UnActiveClick(P1:FindFirstChild("PlayerEsp"):FindFirstChild("activordesc") ,P1:FindFirstChild("PlayerEsp") )

	end
end)
