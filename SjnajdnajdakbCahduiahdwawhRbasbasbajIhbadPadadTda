---------------------TableSave-----------------------
-------------------Save-----------------------------
makefolder("Sun Hub")
local filName = player.Name.."SunHub_Setting.txt"
local filePath = "Sun Hub\\" .. filName

local function LoadSetting()
	local HttpService = game:GetService("HttpService")
	if (readfile and isfile and isfile(filePath)) then
		_G.SaTAble = HttpService:JSONDecode(readfile(filePath))
	end
end
local function saveSetting()
	local Json 
	local HttpService = game:GetService("HttpService")
	if (writefile) then
		Json = HttpService:JSONEncode(_G.SaTAble)
		writefile(filePath ,Json)
	else
		print("Sorry Work Only With Synapse :(")
	end
end

-----------------Save----------------------------------

-----------------EventBar+function----------------------------------
--SpeedBar---
spawn(function()
	local dragging
	local dragInput
	local dragStart
	local startPos
	local startSizeX
	local minSizeX = 0 
	local maxSizeX = 240
	local function update(input , goo)
		local delta = input.Position.X - dragStart.X
		local newSizeX = startSizeX + delta

		-- Vérifie si la taille est dans la plage autorisée
		if newSizeX < minSizeX then
			newSizeX = minSizeX
		elseif newSizeX > maxSizeX then
			newSizeX = maxSizeX
		end

		goo.Size = UDim2.new(goo.Size.X.Scale, newSizeX, goo.Size.Y.Scale, goo.Size.Y.Offset)
	end
	M1:FindFirstChild("BarSpeed").InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startSizeX = M1:FindFirstChild("BarSpeed"):FindFirstChild("ColoreSpeed").Size.X.Offset
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	M1:FindFirstChild("BarSpeed").InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)
	game:GetService("UserInputService").InputChanged:Connect(function(input, gameProcessedEvent)
		if input == dragInput and dragging then
			update(input ,M1:FindFirstChild("BarSpeed"):FindFirstChild("ColoreSpeed"))
		end
	end)

	M1:FindFirstChild("BarSpeed"):FindFirstChild("ColoreSpeed"):GetPropertyChangedSignal("AbsoluteSize"):Connect(function()
		_G.SaTAble.valuepedd = M1:FindFirstChild("BarSpeed"):FindFirstChild("ColoreSpeed").AbsoluteSize.X
		saveSetting()
		wait()
	end)
end)
-----SpeedBar----
