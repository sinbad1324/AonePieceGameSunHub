--------------QuelEstLeNpc----------------
if game.PlaceId == 8396586868 or game.PlaceId == 9432106399 then


	A1:FindFirstChild("AutoFarm"):FindFirstChild("click").MouseButton1Click:Connect(function()
		if _G.NorameTAble.OpenAutofarm == false then
			wait(1)
			A1:FindFirstChild("AutoFarm"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,1,0),0.1)
			A1:FindFirstChild("AutoFarm"):FindFirstChild("ImageLabel").Rotation = 180
			local NPC = {}
			for i , v in pairs(game.ReplicatedStorage:GetChildren()) do 
				if v:FindFirstChild("Data") and v:FindFirstChild("Humanoid") then
					table.insert(NPC , v)
				end
			end
			local unique = {}
			for i , v in pairs(NPC) do 
				local name = v.Name
				if not unique[name] then
					unique[name] = true
					local clone = A1:FindFirstChild("AutoFarm"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):FindFirstChild("Listeisland"):FindFirstChild("NPC"):Clone()
					clone.Parent = A1:FindFirstChild("AutoFarm"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):FindFirstChild("Listeisland")
					clone.Text = v.Name
					clone.Name = v.Name
					clone.Visible = true
					clone.MouseButton1Click:Connect(function()
						A1:FindFirstChild("AutoFarm"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,0,0),0.01)	
						A1:FindFirstChild("AutoFarm"):FindFirstChild("ImageLabel").Rotation = 0
						_G.NorameTAble.OpenAutofarm = false
						A1:FindFirstChild("AutoFarm"):FindFirstChild("statsname").Text = clone.Text
						_G.SaTAble.NPC =  A1:FindFirstChild("AutoFarm"):FindFirstChild("statsname").Text
						saveSetting()
						_G.NorameTAble.OpenAutofarm = false
						spawn(function()
							for i =1,5  do
								if 	_G.NorameTAble.OpenAutofarm == false then
									for i , v in pairs(A1:FindFirstChild("AutoFarm"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame").Listeisland:GetChildren()) do
										if v.ClassName == "TextButton" and v ~= A1:FindFirstChild("AutoFarm"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):FindFirstChild("Listeisland"):FindFirstChild("NPC") then
											v:Destroy()
										end
									end
								end
								wait()
							end
						end)
					end)
				end
			end 
			_G.NorameTAble.OpenAutofarm = true
			wait(0.4)
		else

			wait(0.7)
			A1:FindFirstChild("AutoFarm"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,0,0),0.01)	
			A1:FindFirstChild("AutoFarm"):FindFirstChild("ImageLabel").Rotation = 0
			for i , v in pairs(A1:FindFirstChild("AutoFarm"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame").Listeisland:GetChildren()) do
				if v.ClassName == "TextButton" and v ~= A1:FindFirstChild("AutoFarm"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):FindFirstChild("Listeisland"):FindFirstChild("NPC") then
					v:Destroy()
				end
			end
			_G.NorameTAble.OpenAutofarm = false

		end	
	end)
	--------Open-----------
	--------------QuelEstLeNpc----------------

	----------------------------------------------------Function!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!-------------------------------------------------------

