M1:FindFirstChild("Buttonjump"):FindFirstChild("activordesc").MouseButton1Click:Connect(function()
	if _G.SaTAble.cbcJumb == false then
		if ch then
			local hum = ch.Humanoid
			chnageJumb = hum.Changed:Connect(function()
				hum.UseJumpPower = true
				hum.JumpPower = _G.SaTAble.valueJumb
				ch.HumanoidRootPart.Anchored = false
			end)
			hum.JumpPower = _G.SaTAble.valueJumb
		end
		saveSetting()
		ActiveClcik(M1:FindFirstChild("Buttonjump"):FindFirstChild("activordesc") ,M1:FindFirstChild("Buttonjump") )
		_G.SaTAble.cbcJumb = true
	else
		print()
		chnageJumb:Disconnect()
		local hum = ch.Humanoid
		hum.JumpPower = 50  
		saveSetting()
		UnActiveClick(M1:FindFirstChild("Buttonjump"):FindFirstChild("activordesc") ,M1:FindFirstChild("Buttonjump"))
		_G.SaTAble.cbcJumb = false
	end
end)
