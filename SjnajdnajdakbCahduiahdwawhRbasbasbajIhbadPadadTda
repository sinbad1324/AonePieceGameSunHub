					local ou = R4:FindFirstChild("Auto..."):FindFirstChild("statsname").Text
					xframer2(game.Workspace.ArenaZones[ou].CFrame , 500)
					wait()
				end
			end)
			game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(char2)
				ch = char2
				wait()
				spawn(function()
					while _G.SaTAble.Laquel.arena do
						print()
						if not ch:FindFirstChild("HumanoidRootPart") then
							print()
						elseif ch:FindFirstChild("HumanoidRootPart") then
							local ou = R4:FindFirstChild("Auto..."):FindFirstChild("statsname").Text
							xframer2(game.Workspace.ArenaZones[ou].CFrame , 500)
						end
						wait()
					end
				end)
			end)
		else

			wait(0.5)
			_G.SaTAble.Laquel.arena = false
			saveSetting()
			UnActiveClick(R4:FindFirstChild("ButtonsAuto...."):FindFirstChild("activordesc") ,R4:FindFirstChild("ButtonsAuto....") )
		end	
	end)
