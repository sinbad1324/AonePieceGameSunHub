---------------------------------Function-------------------
local twen 
local twen2 = false
local inf = false


local function NPCRaid()
	local dist, thing = math.huge
	for i, v in pairs(game:GetService("Workspace").Entities:GetChildren()) do
		if v ~= ch then
			local posi = (ch.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude
			if posi < dist then 
				dist = posi 
				thing = v 
			end
		end
	end
	return thing
end

