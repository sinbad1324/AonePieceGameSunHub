local who = P2:FindFirstChild("PlayerToLook"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame").Listeisland.Qui
P2:FindFirstChild("PlayerToLook"):FindFirstChild("click").MouseButton1Click:Connect(function()
	if _G.NorameTAble.Opplayerlook == false then
		_G.NorameTAble.Opplayerlook = true
		wait(1)
		P2:FindFirstChild("PlayerToLook"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,1,0),0.1)
		P2:FindFirstChild("PlayerToLook"):FindFirstChild("ImageLabel").Rotation = 180
		wait(0.1)
		local joueur  = {}
		for i , v in pairs(game.Players:GetPlayers()) do     
			table.insert(joueur , v )
		end 
		for i , v in pairs(joueur) do
			if v ~= game.Players.LocalPlayer and v.Character:FindFirstChild("Humanoid") then
				local clone = who:Clone()
				clone.Text = v.Name
				clone.Parent = who.Parent
				clone.Visible = true
				clone.Name = v.Name
				clone.MouseButton1Click:Connect(function()
					_G.NorameTAble.cbcwhoName = clone.Text
					P2:FindFirstChild("PlayerToLook").statsname.Text = clone.Text
					wait(0.5)
					P2:FindFirstChild("PlayerToLook"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,0,0),0.1)	
					P2:FindFirstChild("PlayerToLook"):FindFirstChild("ImageLabel").Rotation = 0
					_G.NorameTAble.Opplayerlook = false
					spawn(function()
						for i =1,5  do
							if 	_G.NorameTAble.Opplayerlook == false then
								for i , v in pairs(P2:FindFirstChild("PlayerToLook"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame").Listeisland:GetChildren()) do
									if v.ClassName == "TextButton" and v ~= who then
										v:Destroy()
									end
								end
							end 
							wait()
						end
					end)
				end)
			end
		end
	else
		wait(1)
		_G.NorameTAble.Opplayerlook = false
		P2:FindFirstChild("PlayerToLook"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame"):TweenSize(UDim2.new(1.091,0,0,0),0.1)	
		P2:FindFirstChild("PlayerToLook"):FindFirstChild("ImageLabel").Rotation = 0
		for i , v in pairs(P2:FindFirstChild("PlayerToLook"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame").Listeisland:GetChildren()) do
			if v.ClassName == "TextButton" and v ~= who then
				v:Destroy()
			end
		end 
		spawn(function()
			for i =1,5  do
				if 	_G.NorameTAble.Opplayerlook == false then
					for i , v in pairs(P2:FindFirstChild("PlayerToLook"):FindFirstChild("liste"):FindFirstChild("ScrollingFrame").Listeisland:GetChildren()) do
						if v.ClassName == "TextButton" and v ~= who then
							v:Destroy()
						end
					end
				end 
				wait()
			end
		end)
	end	
end)
